<view><cu-custom vue-id="1" bgColor="bg-gradual-blue" isBack="{{true}}" bind:__l="__l" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">选择城市</view></cu-custom><view class="cu-bar bg-white search fixed" style="{{'top:'+(CustomBar+'px')+';'}}"><view class="search-form round"><text class="cuIcon-search"></text><input type="text" placeholder="输入搜索的关键词" confirm-type="search" data-event-opts="{{[['input',[['__set_model',['','searchValue','$event',[]]]]]]}}" value="{{searchValue}}" bindinput="__e"/></view><view class="action"><button data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="cu-btn bg-gradual-blue shadow-blur round" catchtap="__e">搜索</button></view></view><scroll-view class="indexes" style="{{'height:'+('calc(100vh - '+CustomBar+'px - 50px)')+';'}}" scroll-y scroll-into-view="{{'indexes-'+listCurID}}" scroll-with-animation="{{true}}" enable-back-to-top="{{true}}"><view class="location"><view style="height:32px;line-height:32px;padding:0 10px;">定位城市</view><view data-event-opts="{{[['tap',[['selectCity',['$0'],['currentCityCN']]]]]}}" class="position radius shadow bg-white" catchtap="__e">{{''+currentCityCN+''}}</view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="{{['indexItem-'+item.letter]}}" id="{{'indexes-'+item.letter}}" data-index="{{item.letter}}"><block wx:if="{{item.citys.length>0}}"><view class="padding" style="padding:12rpx;">{{item.letter}}</view></block><view class="cu-list menu-avatar no-padding"><block wx:for="{{item.citys}}" wx:for-item="city" wx:for-index="sub" wx:key="sub"><view data-event-opts="{{[['tap',[['selectCity',['$0'],[[['list','',index],['citys','',sub]]]]]]]}}" class="indexes-item cu-item" catchtap="__e"><view class="cityNameCN">{{city.nameCN}}</view><view class="cityNameEN">{{city.nameEN}}</view></view></block></view></view></block></block></scroll-view><view class="indexBar" style="{{'height:'+('calc(100vh - '+CustomBar+'px - 50px)')+';'}}"><view data-event-opts="{{[['touchstart',[['tStart',['$event']]]],['touchend',[['tEnd',['$event']]]],['touchmove',[['tMove',['$event']]]]]}}" class="indexBar-box" bindtouchstart="__e" bindtouchend="__e" catchtouchmove="__e"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="indexBar-item" id="{{item.letter}}" data-event-opts="{{[['touchstart',[['getCur',['$event']]]],['touchend',[['setCur',['$event']]]]]}}" bindtouchstart="__e" bindtouchend="__e">{{''+item.letter}}</view></block></view></view><view hidden="{{!(!hidden)}}" class="indexToast">{{''+listCur+''}}</view><modal-tip class="vue-ref" vue-id="2" data-ref="modaltip" bind:__l="__l"></modal-tip></view>